{
  "version": 2,
  "builds": [
    {
      "src": "src/compliance/mcp_server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/compliance/mcp_server.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  },
  "functions": {
    "src/compliance/mcp_server.py": {
      "maxDuration": 30
    }
  }
}
